<!DOCTYPE html>
<html>
<head>
  <title>Channe Admin</title>
  <%= favicon_link_tag 'Favicon.png' %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
  <div class="fullHeight">
    <div class="admin_nav_div col-sm-8 col-sm-offset-2 text-center">
      <%= link_to("Website", root_path, class: "btn btn-default") %>
      <%= link_to("Admin", admins_path, class: "btn btn-default") %>
  	 <%= link_to((fa_icon "sign-out"), destroy_admin_session_path, :method => :delete, class: "btn btn-default") %>
    </div>
    <div class="container">
      <div class="row text-center ">
  	     <%= yield %>
      </div>
    </div>
  </div>
	<%= render "layouts/footer" %>
</body>
</html>